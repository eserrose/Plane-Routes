{
  "name": "plane-routes",
  "version": "1.0.0",
  "description": "Plane route visualization project using cesium and electron",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 -prune=true --out=release-builds",
    "package-win": "electron-packager . plane-routes --overwrite --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.ProductName=\"Plane Routes\"",    
    "package-linux": "electron-packager . plane-routes --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eserrose/Plane-Routes.git"
  },
  "keywords": [
    "plane",
    "air",
    "traffic",
    "route"
  ],
  "author": "E.Eser Gul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eserrose/Plane-Routes/issues"
  },
  "homepage": "https://github.com/eserrose/Plane-Routes#readme",
  "dependencies": {
    "cesium": "^1.82.1",
    "electron": "^13.1.1",
    "electron-packager": "^15.2.0",
    "electron-serve": "^1.1.0"
  }
}
